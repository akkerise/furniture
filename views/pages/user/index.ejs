<% layout('../../layouts/main') %>
<style>
    .mdi {cursor: pointer;}
</style>
<div class="card">
    <div class="card-body">
        <h4 class="card-title">Users</h4>
        <h6 class="card-subtitle"></h6>
        <div class="table-responsive m-t-40">
            <table id="myTable" class="table table-bordered table-striped">
                <thead>
                <tr>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Expired At</th>
                    <th>Created At</th>
                    <th>Actions</th>
                </tr>
                </thead>
                <tbody>
                    <% data.forEach(user => { %>
                        <tr>
                            <td><%= user.name %></td>
                            <td><%= user.email %></td>
                            <td><%= user.phone %></td>
                            <td><%= user.expiredAt %></td>
                            <td><%= user.createdAt %></td>
                            <td>
                                <i onclick="user.edit(<%= user.id %>)" class="mdi mdi-account-edit"></i>
                                <i onclick="user.del(<%= user.id %>)" class="mdi mdi-delete"></i>
                                <i onclick="user.show(<%= user.id %>)" class="mdi mdi-account"></i>
                            </td>
                        </tr>
                    <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</div>
<% script('/templates/elaadmin/js/custom.min.js') %>
<% script('/templates/elaadmin/js/lib/datatables/datatables.min.js') %>
