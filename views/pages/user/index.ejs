<% script('/templates/elaadmin/js/lib/datatables/cdn.rawgit.com/bpampuch/pdfmake/0.1.18/build/vfs_fonts.js') %>
<% script('/templates/elaadmin/js/lib/datatables/cdn.datatables.net/buttons/1.2.2/js/buttons.html5.min.js') %>
<% script('/templates/elaadmin/js/lib/datatables/cdn.datatables.net/buttons/1.2.2/js/buttons.print.min.js') %>
